{
  
  "Serilog":
  {
    using:  "Serilog.Sinks.File",
    "MinimumLevel": "Information",
    "WriteTo": 
    [
      {
        Name: "File",
        Args:
        {
          "path": "Logs/log-.txt",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    
    "DatetimeFormat": "yyyy-MM-dd HH:mm:ss.fff zzz",
    
    
  }
  


}